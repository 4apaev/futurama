svg#svg-define
  position fixed
  width 0
  height 0
  overflow hidden
  display none

.ico, [class^="ico-"], [class*=" ico-"]
  display inline-block
  width 1em
  height 1em
  stroke-width 0
  // font-size 1em
  stroke currentColor
  fill currentColor


#svg-icons
  .ico
    font-size 2em

  ul
    columns 4
    column-gap 0
    column-rule 1px solid #ccc
    li
      line-height 2
      padding 0 1em
      span
        display inline-block
        pad-l 1em
